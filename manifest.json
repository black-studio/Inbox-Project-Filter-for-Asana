{
  "manifest_version": 3,
  "name": "Inbox Project Filterfor Asana",
  "version": "1.0.0",
  "description": "Filtra automaticamente i progetti Inbox in Asana",
  "author": "Francesco Canovi",
  "homepage_url": "https://www.blackstudio.it",
  "permissions": [
    "webNavigation",
    "scripting"
  ],
  "host_permissions": [
    "https://app.asana.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["https://app.asana.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
