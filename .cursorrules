{
  "description": "Cursor Rules for Inbox Project Filter for Asana Chrome Extension",
  "projectInfo": {
    "overview": "This is a Chrome extension that adds a filter to Asana's Inbox, allowing users to filter notifications by project. The extension injects UI elements into the Asana web app without requiring API access."
  },
  "fileTypes": {
    "js": {
      "tabSize": 4,
      "insertSpaces": true,
      "lineCommentStart": "//"
    },
    "json": {
      "tabSize": 2,
      "insertSpaces": true
    },
    "md": {
      "tabSize": 2,
      "insertSpaces": true
    }
  },
  "codeStyle": {
    "javascript": {
      "quotes": "single",
      "semicolons": true,
      "trailingComma": "es5"
    }
  },
  "extensionRules": {
    "manifestVersion": 3,
    "contentScriptInjection": "declarative",
    "backgroundServiceWorker": true
  },
  "projectStructure": {
    "core": [
      "manifest.json",
      "background.js",
      "content.js"
    ],
    "assets": [
      "icons/"
    ],
    "documentation": [
      "README.md",
      "LICENSE"
    ]
  },
  "namingConventions": {
    "functions": "camelCase",
    "variables": "camelCase",
    "constants": "UPPER_CASE",
    "classes": "PascalCase"
  },
  "documentation": {
    "requireJSDoc": true,
    "requireFunctionComments": true
  },
  "quality": {
    "requireConsistentIndentation": true,
    "noUnusedVariables": true,
    "noImplicitGlobals": true
  }
} 